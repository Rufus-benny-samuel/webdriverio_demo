{
  "name": "webdriverio-test",
  "version": "1.0.0",
  "main": "./build/index.js",
  "scripts": {
    "start": "npx functions-framework --target=inboxSettings [--signature-type=http]",
    "remove": "rm -rf allure-report && rm -rf allure-results",
    "test": "npm run remove && npx wdio wdio.conf.js",
    "multiremote test": "npm run remove && npx wdio wdio.multiremote.conf.js",
    "allure": "allure generate allure-results --clean && allure open",
    "test:report": "jest --coverage",
    "scan": "node sonarqube.js",
    "lint:report": "eslint ./server ./spec --fix -o ./eslint-report.json",
    "lint": "eslint --fix --ext .js,.jsx .",
    "deploy": "gcloud functions deploy CWA  --entry-point inboxSettings --runtime nodejs14 --trigger-http --memory=4096mb --allow-unauthenticated"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.0.0",
    "@types/chai": "^4.3.0",
    "@wdio/allure-reporter": "^7.16.11",
    "@wdio/cli": "^7.16.16",
    "@wdio/cucumber-framework": "^7.16.12",
    "@wdio/local-runner": "^7.16.12",
    "@wdio/mocha-framework": "^7.16.11",
    "@wdio/spec-reporter": "^7.16.11",
    "@wdio/sync": "^7.16.12",
    "allure-commandline": "^2.17.2",
    "async-shelljs": "^0.1.4",
    "chai": "^4.3.4",
    "chai-arrays": "^2.2.0",
    "chrome": "^0.1.0",
    "chrome-aws-lambda": "^10.1.0",
    "chromedriver": "^98.0.0",
    "cucumber-html-reporter": "^5.5.0",
    "cucumberjs-parallel": "^2.0.1",
    "eslint": "^8.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-only-warn": "^1.0.3",
    "expect-webdriverio": "^3.1.4",
    "jest-sonar-reporter": "^2.0.0",
    "sonarqube-scanner": "^2.8.1",
    "wdio-cucumber-parallel-execution": "^3.6.15",
    "wdio-intercept-service": "^4.2.1",
    "wdio-webcomponents": "^1.1.0"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rufus-benny-samuel/webdriverio_demo.git"
  },
  "keywords": [],
  "bugs": {
    "url": "https://github.com/Rufus-benny-samuel/webdriverio_demo/issues"
  },
  "description": "CWA Automation",
  "devDependencies": {
    "@wdio/selenium-standalone-service": "^7.16.11",
    "local-runner": "^0.1.1-alpha",
    "wdio-chromedriver-service": "^7.2.8",
    "webdriverio": "^7.16.16"
  }
}
